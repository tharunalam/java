//THREE IDIOTS

import java.util.*;
public class Three
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int x1=sc.nextInt();
        int y1=sc.nextInt();
        int x2=sc.nextInt();
        int y2=sc.nextInt();
        
        float t=(x1+x2)/2;
        float tt=y1+y2;
        System.out.printf("binoy's house is located at(%.1f,%.1f)",t,tt/2);
        //write your code
    }
}
