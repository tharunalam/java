//sum and difference
import java.util.Scanner;
public class SumDiff
{
    public static void main(String args[])
    {
      Scanner sc=new Scanner(System.in);
      int a=sc.nextInt();
      
      int b= sc.nextInt();
      int temp;
      temp=a+b;
      b=a-b;
      a=temp;
      System.out.println(a);
      System.out.println(" ");      
      System.out.println(b);
      
    
    }
}
