//PRODUCT AND DIVISION//
import java.util.Scanner;
public class ProDiv
{
    public static void main(String args[])
    {
  Scanner sc=new Scanner(System.in);
  int a=sc.nextInt();
  int b=sc.nextInt();
  int c,d,e;
  c=a*b;
  d=a/b;
  e=a%b;
  System.out.println(c);
  System.out.println(d);
  System.out.println(e);
    }
}
