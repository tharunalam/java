//ALICE IN WONDERLA//


import java.util.Scanner;
public class Alice
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=0,c;
        while(a>0){
             c=a%10;
        b=b+c;
        a=a/10;
        
        }
        System.out.printf("%d",b);
    }
}
